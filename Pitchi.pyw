def import_list():
    global active_list
    duplicate_count = 0
    compat_files = [('Pitchi List','*.lst')]
    imported_list = askopenfilename(initialdir=words_l_dir,filetypes=compat_files)
    if not imported_list == '':
        imported_list_info = configparser.ConfigParser()
        imported_list_info.read(imported_list,encoding='utf-8')
        imported_list_words = imported_list_info.sections()
        current_list_info = configparser.ConfigParser()
        list_path = unipath.joinpath(words_l_dir,active_list+'.lst')
        current_list_info.read(list_path,encoding='utf-8')
        current_list_words = current_list_info.sections()
        # Remove duplicate entries
        for i in current_list_words:
            try:
                imported_list_words.remove(i)
                duplicate_count += 1
            except ValueError:
                pass
        imported_list_new_info = configparser.ConfigParser()
        j = 0
        for i in imported_list_words:
            imported_list_new_info.add_section(imported_list_words[j])
            imported_list_new_info.set(imported_list_words[j],'mora',imported_list_info.get(imported_list_words[j],'mora'))
            imported_list_new_info.set(imported_list_words[j],'pap',imported_list_info.get(imported_list_words[j],'pap'))
            imported_list_new_info.set(imported_list_words[j],'romaji',imported_list_info.get(imported_list_words[j],'romaji'))
            imported_list_new_info.set(imported_list_words[j],'translation',imported_list_info.get(imported_list_words[j],'translation'))
            imported_list_new_info.set(imported_list_words[j],'notes',imported_list_info.get(imported_list_words[j],'notes'))
            j += 1
        current_list_file = open(list_path,'a',encoding='utf-8')
        imported_list_new_info.write(current_list_file)
        current_list_file.close()
        # Show Success
        tkMessageBox.showinfo('List imported!',message='List imported succesfully! \nWords added: ' + str(j) + '\nWords already present: '+ str(duplicate_count))
        recount_words()
        populate_side_bar_list()
        word_count_label_var.set('Total words loaded: '+str(word_count))
        settings.lift()
    else:
        settings.lift()
